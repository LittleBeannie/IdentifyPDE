N = seq(1e+1, 2e+5, by = 1e+4)
sigma = c(0.1, 0.25, 0.5, 0.75, 1)
u_norm = 1
lambda_low_bound = NULL
for (i in seq(1, length(sigma))){
lambda_low_bound = c(lambda_low_bound, 4*N*exp(-(N^(16/105) - u_norm)^2/(2*sigma[i]^2))   )
}
data_plot = data.frame(plot_N = rep(N, length(sigma)),
Sigma = as.factor( rep(sigma, each = length(N))),
plot_lambda_low_bound = lambda_low_bound )
library(latex2exp)
library(ggplot2)
library(grDevices)
windowsFonts(myFont=windowsFont("Arial"))
g = ggplot(data_plot, aes(plot_N, plot_lambda_low_bound, group = Sigma))
g = g + geom_line(aes(linetype = Sigma, color = Sigma), size = 1.2)
g = g + geom_point(aes(color = Sigma, shape = Sigma), size = 2)
g = g + theme_bw()
g = g + labs(x = "N", y = "P'")
g = g + theme(axis.title.x = element_text(size = 20, family="myFont", color="black", face= "plain", vjust=0.5, hjust=0.5))
g = g + theme(axis.title.y = element_text(size = 15, family="myFont", color="black", face= "plain", vjust=0.5, hjust=0.5))
g = g + theme(axis.text.x = element_text(size = 15, family="myFont", color="black", face= "plain", vjust=0.5, hjust=0.5))
g = g + theme(axis.text.y = element_text(size = 15, family="myFont", color="black", face= "plain", vjust=0.5, hjust=0.5))
g = g + theme(plot.title = element_text(hjust = 0.5, size = 25, face = "bold") )
